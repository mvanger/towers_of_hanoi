<p id="notice"><%= notice %></p>

<p>
  <b>Text:</b>
  <%= @tweet.text["text"] %></br>
  <b>User:</b>
  @<%= @tweet.text["user"]["screen_name"] %></br>
  <b>Name:</b>
  <%= @tweet.text["user"]["name"] %></br>
  <b>Location:</b>
  <%= @tweet.text["user"]["location"] %></br>
  <b>Retweets:</b>
  <%= @tweet.text["retweet_count"] %></br>
  <b>Favorites:</b>
  <%= @tweet.text["favorite_count"] %></br>
  <b>Language:</b>
  <%= @tweet.text["lang"] %></br>
  <b>Time:</b>
  <%= @tweet.text["created_at"] %></br>
</p>


<%= link_to 'Edit', edit_tweet_path(@tweet) %> |
